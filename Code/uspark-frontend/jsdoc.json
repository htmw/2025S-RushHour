{
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "dist"],
    "includePattern": ".jsx?$"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "outputSourceFiles": true
    },
    "better-docs": {
      "name": "Uspark Documentation",
      "logo": "",
      "title": "Uspark - Project API Documentation",
      "navigation": [
        {
          "label": "Overview",
          "href": "index.html"
        }
      ]
    }
  }
}
